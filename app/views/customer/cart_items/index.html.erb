<h2>ショッピングカート</h2>
<div class="cart-delete">
	<%= link_to "カートを空にする", "#" %>
</div>
<table>
	<thead>
		<tr>
			<th>商品名</th>
			<th>単価（税込）</th>
			<th>数量</th>
			<th>小計</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @cart_items.each do |cart_item| %>
		<% item = cart_item.item %>
		<tr>
			<td></td>
			<td></td>
			<td></td>
			<td></td>
		</tr>
	</tbody>
</table>